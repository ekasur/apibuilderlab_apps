<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <p><label>Current API: <%=currentapi%></label></p>
            <h3>Data details: </h3><br>
            <% data.forEach( function(data) { %>
            <ul class="list-group">
                <li class="list-group-item">Country : <%=data.country_name%> </li>
                <li class="list-group-item">Country Code : <%=data.country_code%> </li>
                <li class="list-group-item">Created date : <%=data.created_datetime%></li>
            </ul>
            <% }); %>
            <br>
            <a href="/">Go Back</a>
        </div>
    </div>
</div>